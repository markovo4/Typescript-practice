# Ignore node_modules directory (for Node.js projects)
node_modules/

# Ignore logs and temporary files
logs/
*.log
npm-debug.log*

# Ignore environment variable files
.env
.env.local

# Ignore build output directories
dist/
build/

# Ignore OS-specific files
.DS_Store      # macOS
Thumbs.db      # Windows

# Ignore IDE/editor config files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# Ignore dependency directories (if any)
vendor/          # for PHP Composer
__pycache__/     # for Python
*.pyc            # for Python bytecode

# Ignore temporary files
*.tmp
*.bak
*.swp
*.temp

# Ignore generated files (such as compiled binaries)
*.exe
*.out
*.dll
*.so
*.dylib
